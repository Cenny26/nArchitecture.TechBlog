@model List<Article>

@{
    ViewData["Title"] = "Home Page";
}

<h1 style="font-size: 20px" class="text-center">We are on Admin area!</h1>
<br/>
<div class="text-center">
    @foreach (var article in Model)
    {
        <h1 style="color: #0c4128">@article.Title</h1>
        <h1 style="font-size: 16px">Creater: @article.CreatedBy</h1>
        <h3 style="color: #842029;">@article.Content</h3>
        
        <br /> <br />
    }
</div>