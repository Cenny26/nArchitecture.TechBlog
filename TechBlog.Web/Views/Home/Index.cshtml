@model List<ArticleDto>

@{
    ViewData["Title"] = "Home Page";
}

<h1 style="font-size: 20px" class="text-center">We are on Default area!</h1>
<br/>
<div class="text-center">
    @foreach (var article in Model)
    {
        <h1 style="color: #0c4128; font-size: 26px;">@article.Title : @article.CreatedBy</h1>
        <h3 style="color: #842029; font-size: 20px;">@article.Content</h3>
        
        <br /> <br />
    }
</div>